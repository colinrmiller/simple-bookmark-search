{
    "name": "Simple Bookmark search",
    "description": "Barebones popup that allows quick bookmark access by name",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "bookmarks"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
      },
    "commands": {
        "open_popup": {
          "suggested_key": {
            "default": "Ctrl+I"
          },
          "description": "Open Bookmark Search Popup"
        }
    }
}
  